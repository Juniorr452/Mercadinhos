<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mobile.pid.pid.feed.FeedActivity">

    <Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimaryDark"
        android:title="@string/nav_feed"
        android:titleTextColor="@color/backgroundLogin">

    </Toolbar>
    <android.support.design.widget.BottomNavigationView
        android:id="@+id/bottom_menu"
        android:layout_width="match_parent"
        android:layout_gravity="bottom"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:itemIconTint="@color/backgroundLogin"
        app:itemTextColor="@color/backgroundLogin"
        android:layout_alignParentStart="true"
        android:visibility="visible"
        android:background="@color/colorPrimaryDark"
        app:menu="@menu/nav_items">


    </android.support.design.widget.BottomNavigationView>
</RelativeLayout>
